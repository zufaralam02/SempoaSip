<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zufaralam02.sempoasip">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.Journey">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.DetailJourney"
            android:parentActivityName=".Activities.Journey" />
        <activity
            android:name=".Activities.Rank"
            android:parentActivityName=".Activities.DetailJourney" />
        <activity
            android:name=".Activities.StageJourney"
            android:parentActivityName=".Activities.DetailJourney" />
        <activity
            android:name=".Activities.NewStageJourney"
            android:parentActivityName=".Activities.DetailJourney" />
    </application>

</manifest>